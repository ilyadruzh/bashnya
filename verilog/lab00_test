#! /usr/local/bin/vvp
:ivl_version "11.0 (stable)" "(v11_0-132-gb2f2414f4)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 10;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
S_0x564a9dc0adb0 .scope module, "testbench" "testbench" 2 3;
 .timescale -9 -10;
v0x564a9dc531f0_0 .var "clk", 0 0;
v0x564a9dc53290_0 .net "clk1", 0 0, L_0x564a9dc53360;  1 drivers
S_0x564a9dc0af40 .scope module, "lab00_test" "lab00_test" 2 12, 3 1 0, S_0x564a9dc0adb0;
 .timescale -9 -10;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /OUTPUT 1 "clk1";
L_0x564a9dc53360 .functor BUFZ 1, v0x564a9dc531f0_0, C4<0>, C4<0>, C4<0>;
v0x564a9dc1e5e0_0 .net "clk", 0 0, v0x564a9dc531f0_0;  1 drivers
v0x564a9dc0bd90_0 .net "clk1", 0 0, L_0x564a9dc53360;  alias, 1 drivers
    .scope S_0x564a9dc0adb0;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564a9dc531f0_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x564a9dc0adb0;
T_1 ;
    %delay 10, 0;
    %load/vec4 v0x564a9dc531f0_0;
    %inv;
    %store/vec4 v0x564a9dc531f0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x564a9dc0adb0;
T_2 ;
    %vpi_call 2 15 "$dumpvars" {0 0 0};
    %vpi_call 2 16 "$display", "Test started..." {0 0 0};
    %delay 100, 0;
    %vpi_call 2 17 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbench.v";
    "lab00_test.v";
